{
  "version": "0.0.1",
  "name": "calendar-test",
  "description": "Calendar Test Example",
  "author": "Vitalii Kolodieiev <uchihaveha@gmail.com> (https://github.com/UchihaVeha)",
  "homepage": "https://github.com/UchihaVeha/calendar-test",
  "repository": {
    "type": "git",
    "url": "https://github.com/UchihaVeha/calendar-test.git"
  },
  "license": "MIT",
  "engines": {
    "node": "8.9.0",
    "npm": "5.5.1"
  },
  "main": "index.js",
  "scripts": {
    "start": "node webpack/webpack-dev-server",
    "test": "npm run lint & npx jest & wait",
    "test:watch": "npx jest --watchAll --verbose",
    "lint": "npx eslint -c .eslintrc src",
    "prettify": "npx prettier --single-quote --trailing-comma none --write \"src/**/*.js\""
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-jss": "^8.1.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.5",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "prettier": "^1.8.1",
    "prop-types": "^15.5.10",
    "raf": "^3.4.0",
    "react-hot-loader": "^3.1.1",
    "react-test-renderer": "^16.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "json",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./testSetup"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/__mocks__",
      "<rootDir>/public",
      "<rootDir>/storybook",
      "<rootDir>/webpack"
    ],
    "globals": {
      "__DEV__": true,
      "__PROD__": false
    }
  }
}
